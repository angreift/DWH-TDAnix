                              name="SqlCommand"
                              UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">set datefirst 7
declare @date as datetime
declare @dateN as datetime
declare @curdate as datetime

set @curdate=convert(varchar(16), dateadd(day, -1, getdate()), 112)

set @date =CASE 
WHEN DATEPART(weekday, @curdate) = 1 THEN @curdate 
WHEN DATEPART(weekday, @curdate) = 2 THEN dateadd(day, + 6, @curdate) 
WHEN DATEPART(weekday, @curdate) = 3 THEN dateadd(day, + 5, @curdate) 
WHEN DATEPART(weekday, @curdate) = 4 THEN dateadd(day, + 4, @curdate) 
WHEN DATEPART(weekday, @curdate) = 5 THEN dateadd(day, + 3, @curdate) 
WHEN DATEPART(weekday, @curdate) = 6 THEN dateadd(day, + 2, @curdate) 
WHEN DATEPART(weekday, @curdate) = 7 THEN dateadd(day, + 1, @curdate)
END
set @dateN = dateadd(dd, -13, @date)

select
 convert(datetime, convert(varchar, dateadd(yyyy,-2000, ЗаказПоставщику._Date_Time), 112)) ДатаДокумента,
 спрСтруктурныеЕдиницы._Code КодМагазина,
 спрНоменклатура._Code КодТовара,
 case when cast(спрКонтрагенты._Folder as int) = 0
        then  спрКонтрагенты._Code
        else  (select top 1  спрКонтрагенты2._Code 
                 from _Reference110 спрКонтрагенты2 
                 where спрКонтрагенты2._IDRRef=спрКонтрагенты._ParentIDRRef)  
 end ИДПоставщика,
 ЗаказПоставщикуСтроки._Fld2983	НеЗаказывать
from _Document2660 ЗаказПоставщику (nolock)
inner join  _Reference129 спрСтруктурныеЕдиницы (nolock) ON спрСтруктурныеЕдиницы._IDRRef = ЗаказПоставщику._Fld2678RRef
inner join _Document2660_VT2682 ЗаказПоставщикуСтроки (nolock) ON ЗаказПоставщикуСтроки._Document2660_IDRRef=ЗаказПоставщику._IDRRef
inner join  _Reference114 спрНоменклатура (nolock) ON спрНоменклатура._IDRRef = ЗаказПоставщикуСтроки._Fld2684RRef 
and спрНоменклатура._Code not like '%:%' and спрНоменклатура._Code &lt;&gt;''
inner join _Reference110 спрКонтрагенты (nolock) ON спрКонтрагенты._IDRRef = ЗаказПоставщикуСтроки._Fld2688RRef

where  dateadd(yyyy,-2000,  ЗаказПоставщику._Date_Time) between @dateN and @date
and   cast(ЗаказПоставщику._Posted as int)=1

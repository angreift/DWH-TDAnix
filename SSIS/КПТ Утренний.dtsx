                            <property
                              dataType="System.String"
                              description="Specifies the SQL statement used by the component to extract data."
                              expressionType="Notify"
                              name="SqlCommand"
                              UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">set datefirst 7

declare @date as datetime
declare @dateN as datetime
declare @curdate as datetime

set @curdate=convert(varchar(16), dateadd(day, -1, getdate()), 112)

set @date =CASE 
WHEN DATEPART(weekday, @curdate) = 1 THEN @curdate 
WHEN DATEPART(weekday, @curdate) = 2 THEN dateadd(day, + 6, @curdate) 
WHEN DATEPART(weekday, @curdate) = 3 THEN dateadd(day, + 5, @curdate) 
WHEN DATEPART(weekday, @curdate) = 4 THEN dateadd(day, + 4, @curdate) 
WHEN DATEPART(weekday, @curdate) = 5 THEN dateadd(day, + 3, @curdate) 
WHEN DATEPART(weekday, @curdate) = 6 THEN dateadd(day, + 2, @curdate) 
WHEN DATEPART(weekday, @curdate) = 7 THEN dateadd(day, + 1, @curdate)
END

set @dateN = dateadd(dd, -13, @date)

select
	convert(datetime, rr50_vt.Дата) Дата,
	спрТовары.code as КодТовара,
	СпрМагазины.code as КодМагазина,
	sum(rr50_vt.sp54) as ОстатокНаНачалоШТ,
	sum(rr50_vt.sp379) as ОстатокНаНачалоРознРуб,
	sum(rr50_vt.ОборотШТ) as ОборотШТ,
	sum(rr50_vt.ОборотРуб) as ОборотРуб
from 
	(
	select
		Календарь2.val Дата,
		rg50_vt.sp51,
		rg50_vt.sp52,
		rg50_vt.sp54,
		rg50_vt.sp379,
		0 ОборотШТ,
		0 ОборотРуб
	from
		dbo.GetTblDate(@dateN, @date) Календарь2
	left join
		rg50 as rg50_vt (nolock)
			on rg50_vt.period=cast((dateadd(m,-1,dateadd(day,1-day(Календарь2.val),Календарь2.val)))as DATETIME)
	union all
	select
		Календарь3.val Дата,
		ra50_vt.sp51,
		ra50_vt.sp52,
		case ra50_vt.debkred when 0 then ra50_vt.sp54 else -ra50_vt.sp54 end,
		case ra50_vt.debkred when 0 then ra50_vt.sp379 else -ra50_vt.sp379 end,
		0,
		0
	from
		dbo.GetTblDate(@dateN, @date) Календарь3
	left join 
		_1sjourn as j50_vt (nolock) 
			on j50_vt.date_time_iddoc &gt; convert(varchar, dateadd(day,1-day(Календарь3.val),Календарь3.val), 112) 
			and j50_vt.date_time_iddoc &lt; (convert(varchar, dateadd(d,-1,Календарь3.val), 112)+'Z')
			and j50_vt.rf50 = 0x1
	left join
		ra50 as ra50_vt (nolock)
			on j50_vt.iddoc = ra50_vt.iddoc
	union all
	select
		Календарь4.val Дата,
		ra50_vt2.sp51,
		ra50_vt2.sp52,
		0,
		0,
		case ra50_vt2.debkred when 0 then ra50_vt2.sp54 else -ra50_vt2.sp54 end,
		case ra50_vt2.debkred when 0 then ra50_vt2.sp379 else -ra50_vt2.sp379 end
	from
		dbo.GetTblDate(@dateN, @date) Календарь4
	left join 
		_1sjourn as j50_vt2 (nolock) 
			on j50_vt2.date_time_iddoc &gt;= convert(varchar, Календарь4.val, 112)
			and j50_vt2.date_time_iddoc &lt; (convert(varchar, Календарь4.val, 112)+'Z')
			and j50_vt2.rf50 = 0x1
	left join
		ra50 as ra50_vt2 (nolock)
			on j50_vt2.iddoc = ra50_vt2.iddoc
	) as rr50_vt
INNER JOIN
   sc11 as СпрТовары (nolock) ON СпрТовары.ID = rr50_vt.sp51
INNER JOIN
   sc36 as СпрМагазины (nolock) ON СпрМагазины.ID = rr50_vt.sp52
where 
СпрМагазины.parentid='     1   '

group by
	convert(datetime, rr50_vt.Дата),
	спрТовары.code,
	СпрМагазины.code</property>

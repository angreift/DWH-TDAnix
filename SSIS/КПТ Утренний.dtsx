                        <component
                          refId="Package\КПТ\ОТБРС\Загрузка матрицы\Загрузка матрицы из ОТБРС (отбор по дате)\ОТБРС"
                          componentClassID="Microsoft.ManagedComponentHost"
                          contactInfo="Extracts data from a relational database by using a .NET provider.;Microsoft Corporation; Microsoft SqlServer v10; © 2007 Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
                          description="Extracts data from a relational database by using a .NET provider."
                          name="ОТБРС"
                          usesDispositions="true"
                          version="4">
                          <properties>
                            <property
                              dataType="System.String"
                              description="Specifies the SQL statement used by the component to extract data."
                              expressionType="Notify"
                              name="SqlCommand"
                              UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">set datefirst 7
declare @date as date
declare @dateN as date
declare @date2N as date
declare @curdate as date
declare @dateNN as date

set @curdate = convert(varchar(16), dateadd(day, -1, getdate()), 112)
set @date =CASE 
  WHEN DATEPART(weekday, @curdate) = 1 THEN @curdate 
  WHEN DATEPART(weekday, @curdate) = 2 THEN dateadd(day, + 6, @curdate) 
  WHEN DATEPART(weekday, @curdate) = 3 THEN dateadd(day, + 5, @curdate) 
  WHEN DATEPART(weekday, @curdate) = 4 THEN dateadd(day, + 4, @curdate) 
  WHEN DATEPART(weekday, @curdate) = 5 THEN dateadd(day, + 3, @curdate) 
  WHEN DATEPART(weekday, @curdate) = 6 THEN dateadd(day, + 2, @curdate) 
  WHEN DATEPART(weekday, @curdate) = 7 THEN dateadd(day, + 1, @curdate)
END
set @dateN = dateadd(dd, -6, @date)
set @date2N = dateadd(dd, -13, @date)

SELECT 
	distinct
	Календарь.[ИД_дата] as Дата,
    ТоварнаяМатрица.Код_магазина,
    ТоварнаяМатрица.Код_товара,
    ТоварнаяМатрица.Код_поставщика,

	(case
		when ТоварнаяМатрица.[Признак] = 3
		then 1
		else 0
	end) as Топ,

	(case 
		when ТоварнаяМатрица.[Код_поставщика] = 1682 
		then 1 
		else 0
	end) as ВидПоставки,

    (select top 1 ТоварнаяМатрица1.Признак
	from td.t_fact_Товарная_матрица as ТоварнаяМатрица1
	where ТоварнаяМатрица1.Код_товара = ТоварнаяМатрица.Код_товара and
	ТоварнаяМатрица1.Код_магазина = ТоварнаяМатрица.Код_магазина and
	ТоварнаяМатрица1.Дата &lt;= @date
	order by ТоварнаяМатрица1.Дата desc) as ВидАсс,

	(select top 1 ТоварнаяМатрица1.Признак
	from td.t_fact_Товарная_матрица as ТоварнаяМатрица1
	where ТоварнаяМатрица1.Код_товара = ТоварнаяМатрица.Код_товара and
	ТоварнаяМатрица1.Код_магазина = ТоварнаяМатрица.Код_магазина and
	ТоварнаяМатрица1.Дата &lt;= @date2N
	order by ТоварнаяМатрица1.Дата desc) as ВидАсс2н

FROM td.t_fact_Товарная_матрица as ТоварнаяМатрица
left join dbo.t_dim_Товары as Товары on Товары.Код_товара = ТоварнаяМатрица.Код_товара
left join [DWH].[dbo].[t_dim_Магазины] as Магазины on Магазины.Код = ТоварнаяМатрица.Код_магазина
cross join [dbo].[t_dim_Календарь] as Календарь

Where
	Магазины.Группа = 'Розница                                 ' and
	(Календарь.[ИД_дата] between @dateN and @date) and
	((select top 1 ТоварнаяМатрица1.Признак
	from td.t_fact_Товарная_матрица as ТоварнаяМатрица1
	where ТоварнаяМатрица1.Код_товара = ТоварнаяМатрица.Код_товара and
	ТоварнаяМатрица1.Код_магазина = ТоварнаяМатрица.Код_магазина and
	ТоварнаяМатрица1.Дата &lt;= @date
	order by ТоварнаяМатрица1.Дата desc) in ('2','3','4','6','9')) and
	((select top 1 ТоварнаяМатрица1.Признак
	from td.t_fact_Товарная_матрица as ТоварнаяМатрица1
	where ТоварнаяМатрица1.Код_товара = ТоварнаяМатрица.Код_товара and
	ТоварнаяМатрица1.Код_магазина = ТоварнаяМатрица.Код_магазина and
	ТоварнаяМатрица1.Дата &lt;= @date2N

                      <properties>
                        <property
                          dataType="System.String"
                          description="Specifies the SQL statement used by the component to extract data."
                          expressionType="Notify"
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">set datefirst 7

declare @date as datetime
declare @dateN as datetime
declare @date5N as datetime
declare @curdate as datetime

set @curdate=convert(varchar(16), dateadd(day, -1, getdate()), 112)

set @date =CASE 
WHEN DATEPART(weekday, @curdate) = 1 THEN @curdate 
WHEN DATEPART(weekday, @curdate) = 2 THEN dateadd(day, + 6, @curdate) 
WHEN DATEPART(weekday, @curdate) = 3 THEN dateadd(day, + 5, @curdate) 
WHEN DATEPART(weekday, @curdate) = 4 THEN dateadd(day, + 4, @curdate) 
WHEN DATEPART(weekday, @curdate) = 5 THEN dateadd(day, + 3, @curdate) 
WHEN DATEPART(weekday, @curdate) = 6 THEN dateadd(day, + 2, @curdate) 
WHEN DATEPART(weekday, @curdate) = 7 THEN dateadd(day, + 1, @curdate)
END
set @dateN = dateadd(dd, -7,@date)
set @date5N = dateadd(dd, -35, @date)

select
	Календарь.val Дата,
	Продажи.КодМагазина,
	case
		when (Продажи.КодТовара &gt; 9999999999) or (Продажи.КодТовара &lt; 0) then 0
		else Продажи.КодТовара
	end as КодТовара,
	Продажи.Количество/4 Количество,
	Продажи.Сумма/4 Сумма
from
	dbo.GetTblDate(@dateN, @date) Календарь
left join
	(SELECT
		DATEPART(weekday ,ch.DATE_TIME_END) ДеньНедели,
		mag.nmag as КодМагазина,
		s.goods_id as КодТовара,
		sum(s.quantity) as Количество,
		sum(s.sum_result) as Сумма
	FROM 
		[olap-rozn].[dbo].[CHEQUES] as ch (nolock)
	inner join 
		dbo.sales as s (nolock)
			on ch.CHEQUE_ID=s.CHEQUE_ID 
	left join 
		dbo.MAGAZIN as mag (nolock)
			on  ch.MAGAZ=mag.MAG 
	where 
		ch.DATE_TIME_END between @date5N and @dateN
		and ch.CHEQUE_TYPE=55
                               and mag.brand&lt;&gt;'ФСК'
	group by 
		mag.nmag,	
		s.goods_id,
		DATEPART(weekday ,ch.DATE_TIME_END)
	) Продажи
		on DATEPART(weekday ,Календарь.val) = Продажи.ДеньНедели</property>

                          description="Specifies the SQL statement used by the component to extract data."
                          expressionType="Notify"
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Select
	ДвиженияСвернуто.Дата
	,ДвиженияСвернуто.[КодТовара]
	,ДвиженияСвернуто.[КодМагазина]
	,ДвиженияСвернуто.[ПлановаяДатаДоставки] 
	,ДвиженияСвернуто.[ФактическаяДатаДоставки]
	,ДвиженияСвернуто.КоличествоЗаказано
	,ДвиженияСвернуто.КоличествоПриход
	,(Select TOP 1 Погрешность.[ПогрешностьКоличество]
		from  [RSC_РегСвед_ДопустимаяПогрешностьКолВаТовара] Погрешность
		left join [Товары] ON [Товары].Код = ДвиженияСвернуто.[КодТовара]
		WHERE Погрешность.[КодГруппыТовара]=[Товары].Код_Группы_Уровень_1
                               order by Погрешность.Дата desc ) ПогрешностьКоличество
	,(Select TOP 1 Погрешность.[ПогрешностьПроцент]
		from  [RSC_РегСвед_ДопустимаяПогрешностьКолВаТовара] Погрешность
		left join [Товары] ON [Товары].Код = ДвиженияСвернуто.[КодТовара]
		WHERE Погрешность.[КодГруппыТовара]=[Товары].Код_Группы_Уровень_1
                               order by Погрешность.Дата desc) ПогрешностьПроцент
FROM
(select Движения.Дата
	,Движения.[КодТовара] [КодТовара]
	,Движения.[КодМагазина] [КодМагазина]
	,Движения.[ПлановаяДатаДоставки] [ПлановаяДатаДоставки]
	,MAX(Движения.[ФактическаяДатаДоставки]) [ФактическаяДатаДоставки]
	,SUM(Движения.КоличествоЗаказано) КоличествоЗаказано
	,SUM(Движения.КоличествоПриход) КоличествоПриход
from (SELECT [ПлановаяДатаДоставки]  Дата
		  ,[КодТовара]
		  ,[КодМагазина]
		  ,[ПлановаяДатаДоставки]
		  ,[ФактическаяДатаДоставки]
		  ,CASE WHEN ВидДвижения = 0 THEN [Количество] ELSE 0 END КоличествоЗаказано
		  ,CASE WHEN ВидДвижения = 1 THEN [Количество] ELSE 0 END КоличествоПриход
	  FROM [KPT].[dbo].[RSC_РасхожденияСПрайдом] (nolock)) Движения
GROUP BY 
                 Движения.Дата
                ,Движения.[КодТовара]
	,Движения.[КодМагазина]
	,Движения.[ПлановаяДатаДоставки]
) ДвиженияСвернуто</property>

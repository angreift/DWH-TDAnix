                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- СТОП-ОТГРУЗКА
	select
		 C3.Дата Дата
		,C3.КодТовара КодТовара
		,C3.КодПоставщика КодПоставщика
		,C3.[КодМагазина] [КодМагазина]

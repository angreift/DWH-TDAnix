                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">set datefirst 7

declare @date as datetime
declare @dateN as datetime
declare @curdate as datetime

set @curdate=convert(varchar(16), dateadd(day, -1, getdate()), 112)

set @date =CASE 
WHEN DATEPART(weekday, @curdate) = 1 THEN @curdate 
WHEN DATEPART(weekday, @curdate) = 2 THEN dateadd(day, + 6, @curdate) 
WHEN DATEPART(weekday, @curdate) = 3 THEN dateadd(day, + 5, @curdate) 
WHEN DATEPART(weekday, @curdate) = 4 THEN dateadd(day, + 4, @curdate) 
WHEN DATEPART(weekday, @curdate) = 5 THEN dateadd(day, + 3, @curdate) 
WHEN DATEPART(weekday, @curdate) = 6 THEN dateadd(day, + 2, @curdate) 
WHEN DATEPART(weekday, @curdate) = 7 THEN dateadd(day, + 1, @curdate)
END

set @dateN = dateadd(dd, -6, @date)

declare @date1c as nchar(9);
declare @dateN1c as nchar(8);

set @date1c = cast(DATEPART(YEAR, @date) as NCHAR(4)) + right('0' + ltrim(rtrim(cast(DATEPART(MONTH, @date) as NCHAR(2)))), 2) + right( '0' + ltrim(rtrim(cast(DATEPART(DAY, @date) as NCHAR(2)))), 2) + 'Z';
set @dateN1c = cast(DATEPART(YEAR, @dateN) as NCHAR(4)) + right('0' + ltrim(rtrim(cast(DATEPART(MONTH, @dateN) as NCHAR(2)))), 2) + right( '0' + ltrim(rtrim(cast(DATEPART(DAY, @dateN) as NCHAR(2)))), 2);


Select
	Distinct
	CAST(LEFT(Жур.Date_Time_IDDoc, 8) as DateTime) as ДатаДок,
	СпрКл.Code as КодМагазина,
	СпрТов.COde as КодТовара,
	cast(1 as tinyint) as Факт
From
	dt5384 ДокС
Inner Join
	dh5384 Док on Док.IDDoc = ДокС.IDDoc and Док.sp5375 in ('    1B   ', '    1Q   ', '    1Y   ') --Переоценка Директора, КО, Скоропорт
Inner Join
	_1SJourn as Жур on Жур.IDDoc = ДокС.IDDoc and
		Жур.Date_Time_IDDoc BETWEEN @dateN1c AND @date1c AND
		Жур.Closed &amp; 1 = 1
Left Join 
	sc36 СпрКл on СпрКл.ID = Док.sp5389
Left Join
	sc11 СпрТов on СпрТов.ID = ДокС.sp5378</property>

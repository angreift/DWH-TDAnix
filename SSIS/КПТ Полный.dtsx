                          description="Specifies the SQL statement used by the component to extract data."
                          expressionType="Notify"
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- Переоценки
	select
		 C3.Дата Дата
		,C3.КодТовара КодТовара
		,C3.КодПоставщика КодПоставщика
		,C3.[КодМагазина] [КодМагазина]
		,0 КПТаутстоков
		,cast(19 as tinyint) КодПричиныПаденияКПТ
		,left(LTRIM(RTRIM( convert(varchar, C3.Дата, 112)))+'_'+
				RTRIM(LTRIM(CAST(C3.КодТовара as CHAR)))+'_'+
		LTRIM(RTRIM( CAST(C3.КодМагазина as CHAR)))+'_'+
		LTRIM(RTRIM( CAST(C3.КодПоставщика as CHAR)))
		,41) ДатаТоварМагазинПоставщик
	from Свод_временный1 C3 with (nolock)
	
	left join ФактПереоценки ФП (nolock) on C3.Дата = ФП.Дата and C3.КодТовара = ФП.КодТовара and C3.КодМагазина = ФП.КодМагазина
	
	where 
 		(case when C3.КолВоДней = null then 0 else Round(C3.ДнейНаСкладеПоОстаткам/C3.КолВоДней*100, 2) end) &lt; 100 and ФП.Факт = 1
 		
 	union all
 	
 -- Уценки ТСД
	select
		 C3.Дата Дата
		,C3.КодТовара КодТовара
		,C3.КодПоставщика КодПоставщика
		,C3.[КодМагазина] [КодМагазина]
		,0 КПТаутстоков
		,cast(20 as tinyint) КодПричиныПаденияКПТ
		,left(LTRIM(RTRIM( convert(varchar, C3.Дата, 112)))+'_'+
				RTRIM(LTRIM(CAST(C3.КодТовара as CHAR)))+'_'+
		LTRIM(RTRIM( CAST(C3.КодМагазина as CHAR)))+'_'+
		LTRIM(RTRIM( CAST(C3.КодПоставщика as CHAR)))
		,41) ДатаТоварМагазинПоставщик
	from Свод_временный1 C3 with (nolock)
	
	left join ФактУценкиТСД ТСД (nolock) on C3.Дата = ТСД.Дата and C3.КодТовара = ТСД.КодТовара and C3.КодМагазина = ТСД.КодМагазина
	

set datefirst 7
declare @date as datetime
declare @dateN as datetime
declare @curdate as datetime

set @curdate=convert(varchar(16), dateadd(day, -1, getdate()), 112)

set @date =CASE 
WHEN DATEPART(weekday, @curdate) = 1 THEN @curdate 
WHEN DATEPART(weekday, @curdate) = 2 THEN dateadd(day, + 6, @curdate) 
WHEN DATEPART(weekday, @curdate) = 3 THEN dateadd(day, + 5, @curdate) 
WHEN DATEPART(weekday, @curdate) = 4 THEN dateadd(day, + 4, @curdate) 
WHEN DATEPART(weekday, @curdate) = 5 THEN dateadd(day, + 3, @curdate) 
WHEN DATEPART(weekday, @curdate) = 6 THEN dateadd(day, + 2, @curdate) 
WHEN DATEPART(weekday, @curdate) = 7 THEN dateadd(day, + 1, @curdate)
END
set @dateN = dateadd(dd, -13, @date)

Select 
 Case  Registr._Period
          When '01.01.2001 00:00:00.000' Then Null
          Else convert(datetime, convert(varchar, dateadd(yyyy,-2000, Registr._Period), 112))  END Дата,
-- Registr._Active	Активность,
-- sys.fn_varbintohexstr(Registr._Active) Активность2,
 спрКонтрагентыРодитель._Code ИДПоставщика,
cast(спрНоменклатура._Code as decimal(10,0)) КодТовара,
 спрСтруктурныеЕдиницы._Code КодМагазина,
 Case  Registr._Fld3026
          When '01.01.2001 00:00:00.000' Then Null
          Else convert(datetime, convert(varchar, dateadd(yyyy,-2000, Registr._Fld3026), 112))  END ДатаЗаказа,
 Case Registr._Fld3027
          When '01.01.2001 00:00:00.000' Then Null
          Else convert(datetime, convert(varchar, dateadd(yyyy,-2000, Registr._Fld3027), 112))  END ФактическаяДатаДоставки,
 Case ПерВидПретензии._EnumOrder 
       When 0 Then 'Недопоставка'
       When 1 Then 'Сверхпоставка'
       When 2 Then 'НеСвоевременнаяПоставка'
       Else 'НеЗадано'
 END ВидПретензии,
-- sys.fn_varbintohexstr(ПерВидПретензии._IDRRef) ПерВидПретензииRef,
 Registr._Fld3028	КоличествоЗаказано,
 Registr._Fld3029	КоличествоПриход,
 Registr._Fld3030	Цена,
 Registr._Fld3879	ОтклонениеВЧасах

from _AccumRg3022 Registr (nolock)
inner join _Reference110 спрКонтрагенты (nolock) ON спрКонтрагенты._IDRRef = Registr._Fld3023RRef
inner join _Reference110 спрКонтрагентыРодитель (nolock) ON спрКонтрагенты._ParentIDRRef = спрКонтрагентыРодитель._IDRRef 
inner join  _Reference114 спрНоменклатура (nolock) ON спрНоменклатура._IDRRef = Registr._Fld3024RRef and спрНоменклатура._Code not like '%:%' and спрНоменклатура._Code &lt;&gt;''
inner join  _Reference129 спрСтруктурныеЕдиницы (nolock) ON спрСтруктурныеЕдиницы._IDRRef = Registr._Fld3025RRef
inner join  _Enum3183 ПерВидПретензии (nolock) ON ПерВидПретензии._IDRRef = Registr._Fld3729RRef 

where  dateadd(yyyy,-2000, Registr._Period) between @dateN and @date
and ( Case ПерВидПретензии._EnumOrder 
       When 0 Then 'Недопоставка'
       When 1 Then 'Сверхпоставка'
       When 2 Then 'НеСвоевременнаяПоставка'
       Else 'НеЗадано'
 END) =  'Недопоставка'</property>

                      <properties>
                        <property
                          dataType="System.String"
                          description="Specifies the SQL statement used by the component to extract data."
                          expressionType="Notify"
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- КОРРЕКТИРОВКА ЗАКАЗА
	select
		 C3.Дата Дата
		,C3.КодТовара КодТовара
		,C3.КодПоставщика КодПоставщика
		,C3.[КодМагазина] [КодМагазина]
		,0 КПТаутстоков
		,cast(11 as tinyint) КодПричиныПаденияКПТ
		,left(LTRIM(RTRIM( convert(varchar, C3.Дата, 112)))+'_'+
				RTRIM(LTRIM(CAST(C3.КодТовара as CHAR)))+'_'+
		LTRIM(RTRIM( CAST(C3.КодМагазина as CHAR)))+'_'+
		LTRIM(RTRIM( CAST(C3.КодПоставщика as CHAR)))
		,41) ДатаТоварМагазинПоставщик
	from Свод_временный1 C3 with (nolock)
	where 
 		(case when C3.КолВоДней = null then 0 else Round(C3.ДнейНаСкладеПоОстаткам/C3.КолВоДней*100, 2) end) &lt; 100 
		and C3.КодТовара	IN (select TOP 1 ОТБРС_КорректировкаЗаказа.КодТовара from ОТБРС_КорректировкаЗаказа with (nolock)
					where  ОТБРС_КорректировкаЗаказа.КодТовара = C3.КодТовара
					AND ОТБРС_КорректировкаЗаказа.КодМагазина = C3.КодМагазина
					AND ОТБРС_КорректировкаЗаказа.Дата&lt;C3.Дата
																	order by ОТБРС_КорректировкаЗаказа.Дата desc)

	Union all
	-- ПРОВЕРИТЬ ЗАКАЗ
	select
		 C3.Дата Дата
		,C3.КодТовара КодТовара
		,C3.КодПоставщика КодПоставщика
		,C3.[КодМагазина] [КодМагазина]
		,0 КПТаутстоков
		,cast(12 as tinyint) КодПричиныПаденияКПТ
		,left(LTRIM(RTRIM( convert(varchar, C3.Дата, 112)))+'_'+
				RTRIM(LTRIM(CAST(C3.КодТовара as CHAR)))+'_'+
		LTRIM(RTRIM( CAST(C3.КодМагазина as CHAR)))+'_'+
		LTRIM(RTRIM( CAST(C3.КодПоставщика as CHAR)))
		,41) ДатаТоварМагазинПоставщик
	from Свод_временный1 C3 with (nolock)
	where 
 		(case when C3.КолВоДней = null then 0 else Round(C3.ДнейНаСкладеПоОстаткам/C3.КолВоДней*100, 2) end) &lt; 100 
		and (Case when (select TOP 1 RSC_Приход.[Количество] from RSC_Приход with (nolock)
								where  RSC_Приход.КодТовара = C3.КодТовара
								AND RSC_Приход.КодМагазина = C3.КодМагазина
								AND convert(varchar(8),RSC_Приход.Дата,112)&lt;C3.Дата
								order by RSC_Приход.Дата desc) is null
				then 1
				else
				 (select TOP 1 RSC_Приход.[Количество]+Остатки.ОстатокНаНачалоШТ from RSC_Приход with (nolock)
								left join ОТБРС_Остатки Остатки (nolock) ON Остатки.Дата = convert(varchar(8),RSC_Приход.Дата ,112)
										and Остатки.КодТовара = RSC_Приход.КодТовара
										and Остатки.КодМагазина = RSC_Приход.КодМагазина
								where  RSC_Приход.КодТовара = C3.КодТовара
								AND RSC_Приход.КодМагазина = C3.КодМагазина
								AND convert(varchar(8),RSC_Приход.Дата,112)&lt;C3.Дата
								order by RSC_Приход.Дата desc)
					- (select SUM(OLAP_Продажи.Количество) from OLAP_Продажи with (nolock) WHERE OLAP_Продажи.КодТовара=C3.КодТовара
											and OLAP_Продажи.КодМагазина=C3.КодМагазина
											and (OLAP_Продажи.ДатаПродажи 
												between (select TOP 1 convert(varchar(8),RSC_Приход.Дата,112) from RSC_Приход with (nolock)
																			where  RSC_Приход.КодТовара = C3.КодТовара

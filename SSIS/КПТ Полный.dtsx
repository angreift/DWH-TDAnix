                  <properties>
                    <property
                      dataType="System.String"
                      description="Specifies the SQL statement used by the component to extract data."
                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	Т1.[Дата]
      ,Т1.[КодТовара]
      ,Т1.[КодПоставщика]
      ,Т1.[КодМагазина]
      ,Т1.[КодПричиныПаденияКПТ]
     , case  when  Т1.[КПТаутстоков] &gt; 0 then Т1.[КПТаутстоков] 
                 when  (Т1.[КПТаутстоков] = 0 and Т1.[КодПричиныПаденияКПТ]=0) then Т1.[КПТаутстоков]

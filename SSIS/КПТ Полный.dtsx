                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">set datefirst 7

declare @date as datetime
declare @dateN as datetime
declare @curdate as datetime
declare @dateNachPred as datetime

set @curdate=convert(varchar(16), dateadd(day, -1, getdate()), 112)

set @date =CASE 
WHEN DATEPART(weekday, @curdate) = 1 THEN @curdate 
WHEN DATEPART(weekday, @curdate) = 2 THEN dateadd(day, + 6, @curdate) 
WHEN DATEPART(weekday, @curdate) = 3 THEN dateadd(day, + 5, @curdate) 
WHEN DATEPART(weekday, @curdate) = 4 THEN dateadd(day, + 4, @curdate) 
WHEN DATEPART(weekday, @curdate) = 5 THEN dateadd(day, + 3, @curdate) 
WHEN DATEPART(weekday, @curdate) = 6 THEN dateadd(day, + 2, @curdate) 
WHEN DATEPART(weekday, @curdate) = 7 THEN dateadd(day, + 1, @curdate)
END
set @dateN = dateadd(dd, -6, @date)
set @dateNachPred = dateadd(dd, -13, @date)

--НАДО ДОБАВИТЬ ГРУППИРОВКУ, Т.к. ВОЗМОЖНЫ ДУБЛИ?

SELECT
 Таблица.Дата, 
 Таблица.КодТовара, 
 Таблица.КодПоставщика, 
 Таблица.КодМагазина,
 Таблица.КодГруппыТовара,
 case when График.КодГруппыТовара is null then 0 else 1 end НаличиеГрафика,
 case when График.ЗапретЗаказаРЦ is null then 0 else 
       (case when График.ЗапретЗаказаРЦ=0 then 0 else 1 end) end ЗапретЗаказаРЦ,
 case when  Таблица.КодГруппыТовара = (select TOP 1 ГрафикСменаПоставщика.КодГруппыТовара
					from ОТБРС_ГрафикЗаказаКратко ГрафикСменаПоставщика
					where ГрафикСменаПоставщика.КодГруппыТовара = Таблица.КодГруппыТовара
					and ГрафикСменаПоставщика.КодМагазина = Таблица.КодМагазина
					and ГрафикСменаПоставщика.КодПоставщика = Таблица.КодПоставщика
					and ГрафикСменаПоставщика.Режим = 2
					and ГрафикСменаПоставщика.ДатаСменыПоставщика	between @dateNachPred and @date) then 1
	else 0 end СменаПоставщика

FROM
(SELECT    
 Свод.Дата Дата, 
 Свод.КодТовара КодТовара, 
 Свод.КодПоставщика КодПоставщика, 
 Свод.КодМагазина КодМагазина,
 Case WHEN Товары2.Родитель IS NUll Then Товары2.Код ELSE Товары1.Код END КодГруппыТовара
FROM       Свод
LEFT JOIN Товары Товары3 ON  Товары3.Код = Свод.КодТовара 
LEFT JOIN Товары Товары2 ON  Товары2.Код = Товары3.Родитель
LEFT JOIN Товары Товары1 ON  Товары1.Код = Товары2.Родитель
Where     Свод.Дата between @dateN and @date) Таблица

Left join ОТБРС_ГрафикЗаказаКратко График ON График.КодГруппыТовара = Таблица.КодГруппыТовара
   AND График.КодМагазина = Таблица.КодМагазина AND  График.КодПоставщика = Таблица.КодПоставщика</property>

                      <properties>
                        <property
                          dataType="System.String"
                          description="Specifies the SQL statement used by the component to extract data."
                          expressionType="Notify"
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
		 Свод_временный1.Дата Дата
		,Свод_временный1.КодТовара КодТовара
		,Свод_временный1.КодПоставщика КодПоставщика
		,Свод_временный1.[КодМагазина] [КодМагазина]
		,cast(Потери.КПТост as numeric(6,3)) КПТаутстоков
		,Потери.Причина КодПричиныПаденияКПТ
		,left(LTRIM(RTRIM( convert(varchar, Свод_временный1.Дата, 112)))+'_'+
				RTRIM(LTRIM(CAST(Свод_временный1.КодТовара as CHAR)))+'_'+
		LTRIM(RTRIM( CAST(Свод_временный1.КодМагазина as CHAR)))+'_'+
		LTRIM(RTRIM( CAST(Свод_временный1.КодПоставщика as CHAR)))
		,41) ДатаТоварМагазинПоставщик
	from Свод_временный1 with (nolock)
	left join 
	(select
		C2.Дата Дата,
		C2.КодТовара КодТовара,
		C2.КодМагазина КодМагазина,
		C2.КолВоДней КолВоДней,
		C2.ДнейНаСкладеПоОстаткам ДнейНаСкладеПоОстаткам,
		C2.КодПоставщика,
		case when C2.КолВоДней = null then 0 else Round(C2.ДнейНаСкладеПоОстаткам/C2.КолВоДней,3) end КПТост,
		0 Причина
		from Свод_временный1 C2 with (nolock)
	 Union all
	 -- ПОТЕРИ - ВОЗВРАТЫ
	 --  WHEN ДокПотери.sp1832 IN ('36','37','38') THEN 'Возвраты'
	 --  WHEN ДокПотери.sp1832 IN ('42','19','17') THEN 'Инвентаризации'
	 --    WHEN ДокПотери.sp1832 IN ('25') THEN 'НепродаваемыйТовар'
	 select
		C3.Дата Дата,
		C3.КодТовара КодТовара,
		C3.КодМагазина КодМагазина,
		C3.КолВоДней КолВоДней,
		C3.ДнейНаСкладеПоОстаткам ДнейНаСкладеПоОстаткам,
		C3.КодПоставщика,
		0 КПТост,
		6 Причина
	 from Свод_временный1 C3 with (nolock)
	 where (case when C3.КолВоДней = null then 0 else Round(C3.ДнейНаСкладеПоОстаткам/C3.КолВоДней*100, 2) end) &lt; 100 
		and C3.КодТовара IN (select TOP 1 ОТБРС_Потери.КодТовара from ОТБРС_Потери with (nolock)
								where  ОТБРС_Потери.КодТовара = C3.КодТовара
								AND ОТБРС_Потери.Дата = C3.Дата
								AND ОТБРС_Потери.КодМагазина = C3.КодМагазина
								AND  ОТБРС_Потери.КодПричины IN ('36','37','38')) 
	 Union all
	 -- ПОТЕРИ - Инвентаризации
	 select
		C3.Дата Дата,
		C3.КодТовара КодТовара,
		C3.КодМагазина КодМагазина,
		C3.КолВоДней КолВоДней,
		C3.ДнейНаСкладеПоОстаткам ДнейНаСкладеПоОстаткам,
		C3.КодПоставщика,
		0 КПТост,
		7 Причина
	 from Свод_временный1 C3 with (nolock)
	 where (case when C3.КолВоДней = null then 0 else Round(C3.ДнейНаСкладеПоОстаткам/C3.КолВоДней*100, 2) end) &lt; 100 
		  and  C3.КодТовара IN (select TOP 1 ОТБРС_Потери.КодТовара from ОТБРС_Потери with (nolock)
								where  ОТБРС_Потери.КодТовара = C3.КодТовара
								AND ОТБРС_Потери.Дата &lt;= C3.Дата
								AND ОТБРС_Потери.КодМагазина = C3.КодМагазина
								AND  ОТБРС_Потери.КодПричины IN ('42','19','17')
								and ОТБРС_Потери.Количество &lt; 0
								order by ОТБРС_Потери.Дата desc)	 
		--проверяем приходы после инвентаризации для следующих дней
		and (case when (select TOP 1 ОТБРС_Потери.Дата
						from ОТБРС_Потери with (nolock)
						 where ОТБРС_Потери.КодТовара = C3.КодТовара
						 AND ОТБРС_Потери.Дата &lt;= C3.Дата
						 AND ОТБРС_Потери.КодМагазина = C3.КодМагазина
						 AND  ОТБРС_Потери.КодПричины IN ('42','19','17')
						 and ОТБРС_Потери.Количество &lt; 0
						 order by ОТБРС_Потери.Дата desc
						 ) is not null 
				then
				isnull((select SUM(RSC_Приход.Количество)
						from RSC_Приход with (nolock)
						 where RSC_Приход.КодТовара = C3.КодТовара
						AND RSC_Приход.КодМагазина = C3.КодМагазина
						AND RSC_Приход.Дата &lt; C3.Дата--без равенства, т.к. в этот день поставка не должна учитываться
						AND RSC_Приход.Дата &gt;= (select TOP 1 ОТБРС_Потери.Дата
												from ОТБРС_Потери with (nolock)
												 where ОТБРС_Потери.КодТовара = C3.КодТовара
												 AND ОТБРС_Потери.Дата &lt;= C3.Дата
												 AND ОТБРС_Потери.КодМагазина = C3.КодМагазина
												 AND  ОТБРС_Потери.КодПричины IN ('42','19','17')
												 and ОТБРС_Потери.Количество &lt; 0
												 order by ОТБРС_Потери.Дата desc
														 )),0)
				else 0
				end) = 0
		
	Union all
	 -- ПОТЕРИ - НепродаваемыйТовар
	 select
		C3.Дата Дата,
		C3.КодТовара КодТовара,
		C3.КодМагазина КодМагазина,
		C3.КолВоДней КолВоДней,
		C3.ДнейНаСкладеПоОстаткам ДнейНаСкладеПоОстаткам,
		C3.КодПоставщика,
		0 КПТост,
		8 Причина
	 from Свод_временный1 C3 with (nolock)
	 where (case when C3.КолВоДней = null then 0 else Round(C3.ДнейНаСкладеПоОстаткам/C3.КолВоДней*100, 2) end) &lt; 100 	 
				and C3.КодТовара IN (select TOP 1 ОТБРС_Потери.КодТовара from ОТБРС_Потери with (nolock)
								where  ОТБРС_Потери.КодТовара = C3.КодТовара
								AND ОТБРС_Потери.Дата = C3.Дата
								AND ОТБРС_Потери.КодМагазина = C3.КодМагазина
								AND ОТБРС_Потери.КодПричины IN ('25'))
	 ) Потери
																			ON Потери.Дата = Свод_временный1.Дата 
																			AND Потери.КодТовара =Свод_временный1.КодТовара 
																			AND Потери.КодМагазина = Свод_временный1.КодМагазина 
																			AND Потери.КодПоставщика = Свод_временный1.КодПоставщика</property>

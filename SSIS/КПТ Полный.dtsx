                          expressionType="Notify"
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">declare @Percent as decimal(12,2)
	set @Percent = (Select TOP 1 ПороговоеЗначение From ВидыПричинПаденияКПТ where Code=16) 
   Select
		Данные.ДатаПродажи
		,Данные.КодМагазина
		,Данные.КодТовара
		,Данные.ПроданоШТ
		,Данные.СреднедневныеПродажиШТ
		,Данные.ПроцентОтклоненияОтСреднихПродаж
    From (Select
		Продажи.ДатаПродажи
		,Продажи.КодМагазина
		,Продажи.КодТовара
		,Продажи.ПроданоШТ
		,Среднедневные.[Количество] СреднедневныеПродажиШТ
		,case when Среднедневные.[Количество] = 0 then null	
				else (case when (ROUND(Продажи.ПроданоШТ/Среднедневные.[Количество]*100,2)&gt;10000000) then 10000000 else
					ROUND(Продажи.ПроданоШТ/Среднедневные.[Количество]*100,2) end) end ПроцентОтклоненияОтСреднихПродаж
	From(
	SELECT [ДатаПродажи]
		  ,[КодМагазина]
		  ,[КодТовара]
		  ,Sum([Количество]) ПроданоШТ
		  ,Sum([Сумма]) ПроданоРуб
	  FROM [OLAP_Продажи] with (nolock)
	  group by 
			[ДатаПродажи]
		  ,[КодМагазина]
		  ,[КодТовара]) Продажи
	 Left join (SELECT [Дата]
		  ,[КодМагазина]
		  ,[КодТовара]
		  ,[Количество]
		  ,[Сумма]
	  FROM [СреднедневныеПоДнямНедели] with (nolock)) Среднедневные ON Среднедневные.[Дата]=Продажи.ДатаПродажи
						AND Среднедневные.[КодМагазина]=Продажи.[КодМагазина] AND Среднедневные.[КодТовара]=Продажи.[КодТовара]) Данные
	Where ПроцентОтклоненияОтСреднихПродаж&gt;@Percent</property>

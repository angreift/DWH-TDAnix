                        <property
                          dataType="System.String"
                          description="Specifies the SQL statement used by the component to extract data."
                          expressionType="Notify"
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">set datefirst 7

declare @date as datetime
declare @dateN as datetime
declare @curdate as datetime

set @curdate=convert(varchar(16), dateadd(day, -1, getdate()), 112)

set @date =CASE 
WHEN DATEPART(weekday, @curdate) = 1 THEN @curdate 
WHEN DATEPART(weekday, @curdate) = 2 THEN dateadd(day, + 6, @curdate) 
WHEN DATEPART(weekday, @curdate) = 3 THEN dateadd(day, + 5, @curdate) 
WHEN DATEPART(weekday, @curdate) = 4 THEN dateadd(day, + 4, @curdate) 
WHEN DATEPART(weekday, @curdate) = 5 THEN dateadd(day, + 3, @curdate) 
WHEN DATEPART(weekday, @curdate) = 6 THEN dateadd(day, + 2, @curdate) 
WHEN DATEPART(weekday, @curdate) = 7 THEN dateadd(day, + 1, @curdate)
END

set @dateN = dateadd(dd, -13, @date)

SELECT 
 Cast(left(Жур.Date_Time_IDDoc,8) as datetime) as Дата,
 спрКлиенты.Code as КодМагазина,
 спрТовары.Code as КодТовара,
 спрВидыНаценки.Code as КодПричины,
 ДокАктСписания.sp5333 Количество,
 ДокАктСписания.sp5335 as Сумма
FROM 
 [dt5341] as ДокАктСписания (NoLock)
INNER JOIN
 [_1SJourn] as Жур (NoLock) ON Жур.IDDoc=ДокАктСписания.IDDoc AND
              (cast(LEFT(Жур.DATE_TIME_IDDOC, 8) as datetime) between @dateN and @date) AND
             Жур.Closed &amp; 1 = 1   
LEFT JOIN [sc11] спрТовары (NoLock) On спрТовары.ID = ДокАктСписания.sp5332
LEFT JOIN [dh5341] as ДокАктСписания_Шапка (NoLock) ON ДокАктСписания_Шапка.IDDoc=ДокАктСписания.IDDoc
LEFT JOIN [sc36] спрКлиенты (NoLock) On спрКлиенты.ID = ДокАктСписания_Шапка.sp5368
LEFT JOIN [sc1705] спрВидыНаценки (NoLock) On спрВидыНаценки.ID = ДокАктСписания_Шапка.sp5331</property>

                          expressionType="Notify"
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- КВАНТ-ОТГРУЗКИ
	select
		 C3.Дата Дата
		,C3.КодТовара КодТовара
		,C3.КодПоставщика КодПоставщика
		,C3.[КодМагазина] [КодМагазина]
		,0 КПТаутстоков
		,cast(18 as tinyint) КодПричиныПаденияКПТ
		,left(LTRIM(RTRIM( convert(varchar, C3.Дата, 112)))+'_'+
				RTRIM(LTRIM(CAST(C3.КодТовара as CHAR)))+'_'+
		LTRIM(RTRIM( CAST(C3.КодМагазина as CHAR)))+'_'+
		LTRIM(RTRIM( CAST(C3.КодПоставщика as CHAR)))
		,41) ДатаТоварМагазинПоставщик
	from Свод_временный1 C3 with (nolock)
	where 
 		(case when C3.КолВоДней = null then 0 else Round(C3.ДнейНаСкладеПоОстаткам/C3.КолВоДней*100, 2) end) &lt; 100 
		and C3.КодТовара IN (select top 1 RSC_НезаказанныйТоварПоПричинам.КодТовара from RSC_НезаказанныйТоварПоПричинам with (nolock)

                          description="Specifies the SQL statement used by the component to extract data."
                          expressionType="Notify"
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT О1.[Дата]
      ,О1.[КодТовара]
      ,О1.[КодМагазина]
      ,О1.[ОстатокНаНачалоШТ]+О1.[ОборотШТ] ОстатокНаКонец
      ,(select top 1 О2.Дата from [ОТБРС_Остатки] О2 where 
				О2.КодМагазина = О1.КодМагазина
				and О2.КодТовара = О1.КодТовара
				and О2.Дата&lt;О1.[Дата] 
				and (О2.[ОстатокНаНачалоШТ]+О2.[ОборотШТ]) &gt; 0 
				order by О2.Дата desc) ДатаПоложительногоОстатка
	  ,case when (select top 1 О2.Дата from [ОТБРС_Остатки] О2 where 
				О2.КодМагазина = О1.КодМагазина
				and О2.КодТовара = О1.КодТовара
				and О2.Дата&lt;О1.[Дата] 
				and (О2.[ОстатокНаНачалоШТ]+О2.[ОборотШТ]) &gt; 0 
				order by О2.Дата desc) IS NULL THEN (select SUM(Списания.Количество) From ОТБРС_Списания Списания
														where Списания.КодМагазина = О1.КодМагазина
														and Списания.КодТовара = О1.КодТовара
														and Списания.Дата &lt;= О1.Дата)
			ELSE (select SUM(Списания.Количество) From ОТБРС_Списания Списания
														where Списания.КодМагазина = О1.КодМагазина
														and Списания.КодТовара = О1.КодТовара
														and Списания.Дата &lt;= О1.Дата
														and Списания.Дата &gt; ((select top 1 О2.Дата from [ОТБРС_Остатки] О2 where 
				О2.КодМагазина = О1.КодМагазина
				and О2.КодТовара = О1.КодТовара
				and О2.Дата&lt;О1.[Дата] 
				and (О2.[ОстатокНаНачалоШТ]+О2.[ОборотШТ])&gt; 0
                                                  order by О2.Дата desc))) END КолСписаноПослеПоложительногоОстатка
			
  FROM [KPT].[dbo].[ОТБРС_Остатки] О1
  where 
   [ОстатокНаНачалоШТ]+[ОборотШТ] &lt;= 0</property>

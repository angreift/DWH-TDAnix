              SQLTask:SqlStatementSource="&#x9;SET NOCOUNT ON;&#xA;    --Удаление лишних причины &quot;Сводные потери&quot; (код 10) в таблице Свод_временный2&#xA;&#x9;delete from Свод_временный2&#xA;&#x9;where  КодПричиныПаденияКПТ=10&#xA;&#x9;&#x9;and ДатаТоварМагазинПоставщик IN (select T.ДатаТоварМагазинПоставщик &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;from Свод_временный2 as T  &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;where T.КодПричиныПаденияКПТ&gt;0 and T.КодПричиныПаденияКПТ&lt;&gt;10 )&#xA;&#x9;&#xA;&#x9;--Удалить причину &quot;Проверить заказ&quot; (12) если есть причина &quot;Не заказывать&quot; (13)&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;delete from Свод_временный2&#xA;&#x9;where  КодПричиныПаденияКПТ=12 and ДатаТоварМагазинПоставщик IN (select T.ДатаТоварМагазинПоставщик &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;from Свод_временный2 as T  &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;where T.КодПричиныПаденияКПТ=13)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;--Установка приоритетов&#xA;&#x9;update [Свод_временный2]&#xA;&#x9;Set [Приоритет] = (select TOP 1 Виды.Приоритет from dbo.ВидыПричинПаденияКПТ Виды where Виды.Code=[Свод_временный2].КодПричиныПаденияКПТ)&#xA;&#xA;&#x9;--Заполнение таблицы максимальных приоритетов&#xA;&#x9;truncate table Свод_временный_приоритеты&#xA;    insert Свод_временный_приоритеты&#xA;&#x9;  (&#xA;&#x9;&#x9;&#x9;[Дата]&#xA;&#x9;&#x9;  ,[КодТовара]&#xA;&#x9;&#x9;  ,[КодПоставщика]&#xA;&#x9;&#x9;  ,[КодМагазина]&#xA;&#x9;&#x9;  ,[КодПричиныПаденияКПТ]&#xA;&#x9;&#x9;  ,[КПТаутстоков]&#xA;&#x9;&#x9;  ,[ДатаТоварМагазинПоставщик]&#xA;&#x9;&#x9;  ,[Приоритет]&#xA;&#x9;&#x9;  ,[ПриоритетМаксимальный]&#xA;&#x9;  )&#xA;&#x9;  select &#xA;&#x9;&#x9;&#x9;T1.[Дата]&#xA;&#x9;&#x9;  ,T1.[КодТовара]&#xA;&#x9;&#x9;  ,T1.[КодПоставщика]&#xA;&#x9;&#x9;  ,T1.[КодМагазина]&#xA;&#x9;&#x9;  ,T1.[КодПричиныПаденияКПТ]&#xA;&#x9;&#x9;  ,T1.[КПТаутстоков]&#xA;&#x9;&#x9;  ,T1.[ДатаТоварМагазинПоставщик]&#xA;&#x9;&#x9;  ,T1.[Приоритет]&#xA;&#x9;&#x9;  ,(select MAX(isnull(T2.Приоритет,0)) from Свод_временный2 T2 where T2.ДатаТоварМагазинПоставщик = T1.ДатаТоварМагазинПоставщик)&#xA;&#x9; from Свод_временный2 T1&#xA;&#x9; &#xA;&#x9;--удаление причин с меньшим приоритетом&#x9; &#xA;   delete FROM [KPT].[dbo].[Свод_временный_приоритеты]&#xA;   where [КодПричиныПаденияКПТ]&lt;&gt;0 and [Приоритет]&lt;[ПриоритетМаксимальный]" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />

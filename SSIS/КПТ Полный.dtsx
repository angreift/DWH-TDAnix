                        <property
                          dataType="System.String"
                          description="Specifies the SQL statement used by the component to extract data."
                          expressionType="Notify"
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">set datefirst 7

declare @date as datetime
declare @dateN as datetime
declare @curdate as datetime

set @curdate=convert(varchar(16), dateadd(day, -1, getdate()), 112)

set @date =CASE 
WHEN DATEPART(weekday, @curdate) = 1 THEN @curdate 
WHEN DATEPART(weekday, @curdate) = 2 THEN dateadd(day, + 6, @curdate) 
WHEN DATEPART(weekday, @curdate) = 3 THEN dateadd(day, + 5, @curdate) 
WHEN DATEPART(weekday, @curdate) = 4 THEN dateadd(day, + 4, @curdate) 
WHEN DATEPART(weekday, @curdate) = 5 THEN dateadd(day, + 3, @curdate) 
WHEN DATEPART(weekday, @curdate) = 6 THEN dateadd(day, + 2, @curdate) 
WHEN DATEPART(weekday, @curdate) = 7 THEN dateadd(day, + 1, @curdate)
END

set @dateN = dateadd(dd, -60, @date)

select
	Журнал.DocNo НомерДокумента,
	ШапкаПриказа.SP1761 НомерПриказа,
	cast(LEFT(Журнал.DATE_TIME_IDDOC, 8) as datetime) Дата,
	СправочникВидыАкций.Code КодВидаАкции,
	ШапкаПриказа.SP1762 ДатаНачалаАкции,
	ШапкаПриказа.SP1763 ДатаКонцаАкции,
	СправочникКлиенты.Code КодМагазина,
	СправочникТовары.Code КодТовара
from [DT1769] СтрокиПриказа (nolock)
left join [DH1769] ШапкаПриказа (nolock)
	on ШапкаПриказа.IDDOC = СтрокиПриказа.IDDOC
left join [SC2335] СправочникВидыАкций (nolock)	
	ON СправочникВидыАкций.ID = ШапкаПриказа.SP1764
inner join [_1SJOURN] Журнал (nolock)
	ON Журнал.Closed = 1
	AND Журнал.IDDOC = ШапкаПриказа.IDDOC
	AND (cast(LEFT(Журнал.DATE_TIME_IDDOC, 8) as datetime) between @dateN and @date)
inner join [SC3548] СписокМагазинов (nolock)
	ON СписокМагазинов.SP3546 = ШапкаПриказа.IDDOC
left join [SC36] СправочникКлиенты (nolock)
	ON СписокМагазинов.SP3545 = СправочникКлиенты.ID
left join [SC11] СправочникТовары (nolock)
	ON СтрокиПриказа.SP1766 = СправочникТовары.ID
Where
	LTRIM(RTRIM(СправочникВидыАкций.Code)) IN ('3','7','8', '9', '10','15','18')</property>

		OLAP_Продажи.ДатаПродажи,
		OLAP_Продажи.КодТовара,
		OLAP_Продажи.КодМагазина,
		sum(OLAP_Продажи.Количество) Количество,
		sum(OLAP_Продажи.Сумма) Сумма
	from
		RSC_Приход (nolock)
	left join
		OLAP_Продажи (nolock)
			on OLAP_Продажи.ДатаПродажи = RSC_Приход.ДатаПриемки
			and OLAP_Продажи.ВремяПродажи &lt; RSC_Приход.ВремяПриемки
			and OLAP_Продажи.КодТовара = RSC_Приход.КодТовара
			and OLAP_Продажи.КодМагазина = RSC_Приход.КодМагазина
	group by
		OLAP_Продажи.ДатаПродажи,
		OLAP_Продажи.КодТовара,
		OLAP_Продажи.КодМагазина
	) OLAP_ПродажиДоПрихода
		on OLAP_ПродажиДоПрихода.ДатаПродажи = ОТБРС_Матрица.Дата
		and OLAP_ПродажиДоПрихода.КодТовара = ОТБРС_Матрица.КодТовара
		and OLAP_ПродажиДоПрихода.КодМагазина = ОТБРС_Матрица.КодМагазина
left join
	Среднедневные (nolock)
		on Среднедневные.Дата = ОТБРС_Матрица.Дата
		and Среднедневные.КодМагазина = ОТБРС_Матрица.КодМагазина
		and Среднедневные.КодТовара = ОТБРС_Матрица.КодТовара
left join
	СреднедневныеПоДнямНедели (nolock)
		on СреднедневныеПоДнямНедели.Дата = ОТБРС_Матрица.Дата
		and СреднедневныеПоДнямНедели.КодМагазина = ОТБРС_Матрица.КодМагазина
		and СреднедневныеПоДнямНедели.КодТовара = ОТБРС_Матрица.КодТовара
left join
	Магазины (nolock) on Магазины.Код = ОТБРС_Матрица.КодМагазина

left join
	dbo.Товары as спрТовары  (nolock) ON спрТовары.Код=ОТБРС_Матрица.КодТовара

---- ОСТАТКИ ГЛ для АВТОЗАКАЗА
left join
	dbo.Товары as ТоварГлДляАвтозака (nolock) ON ТоварГлДляАвтозака.Код=ОТБРС_Матрица.КодТовара and ТоварГлДляАвтозака.Код&lt;&gt;ТоварГлДляАвтозака.КодГлавныйДляАвтозаказа
left join
	ОТБРС_Остатки as ОТБРС_Остатки_ГлДляАвтозаказа (nolock)
		on ОТБРС_Остатки_ГлДляАвтозаказа.Дата = ОТБРС_Матрица.Дата
		and ОТБРС_Остатки_ГлДляАвтозаказа.КодТовара = ТоварГлДляАвтозака.КодГлавныйДляАвтозаказа
		and ОТБРС_Остатки_ГлДляАвтозаказа.КодМагазина = ОТБРС_Матрица.КодМагазина
left join
	(select
		OLAP_Продажи.ДатаПродажи,
		OLAP_Продажи.КодТовара,
		OLAP_Продажи.КодМагазина,
		sum(OLAP_Продажи.Количество) Количество,
		sum(OLAP_Продажи.Сумма) Сумма
	from
		RSC_Приход (nolock)
	left join
		OLAP_Продажи (nolock)
			on OLAP_Продажи.ДатаПродажи = RSC_Приход.ДатаПриемки
			and OLAP_Продажи.ВремяПродажи &lt; RSC_Приход.ВремяПриемки
			and OLAP_Продажи.КодТовара = RSC_Приход.КодТовара
			and OLAP_Продажи.КодМагазина = RSC_Приход.КодМагазина
	group by
		OLAP_Продажи.ДатаПродажи,
		OLAP_Продажи.КодТовара,
		OLAP_Продажи.КодМагазина
	) OLAP_ПродажиДоПрихода_ГлДляАвтозаказа
		on OLAP_ПродажиДоПрихода_ГлДляАвтозаказа.ДатаПродажи = ОТБРС_Матрица.Дата
		and OLAP_ПродажиДоПрихода_ГлДляАвтозаказа.КодТовара = ТоварГлДляАвтозака.КодГлавныйДляАвтозаказа
		and OLAP_ПродажиДоПрихода_ГлДляАвтозаказа.КодМагазина = ОТБРС_Матрица.КодМагазина

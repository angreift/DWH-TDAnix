declare @curdate as datetime

set @curdate=convert(varchar(16), dateadd(day, -1, getdate()), 112)

set @date =CASE 
WHEN DATEPART(weekday, @curdate) = 1 THEN @curdate 
WHEN DATEPART(weekday, @curdate) = 2 THEN dateadd(day, + 6, @curdate) 
WHEN DATEPART(weekday, @curdate) = 3 THEN dateadd(day, + 5, @curdate) 
WHEN DATEPART(weekday, @curdate) = 4 THEN dateadd(day, + 4, @curdate) 
WHEN DATEPART(weekday, @curdate) = 5 THEN dateadd(day, + 3, @curdate) 
WHEN DATEPART(weekday, @curdate) = 6 THEN dateadd(day, + 2, @curdate) 
WHEN DATEPART(weekday, @curdate) = 7 THEN dateadd(day, + 1, @curdate)
END
set @dateN = dateadd(dd, -13, @date)
set @date= dateadd(dd, +1, @date) --учтем время в дате, прибавим день

SELECT
	dateadd(yyyy,-2000,doc.[_Date_Time]) as Дата,
	doc._Fld3069 НомерВМагазине,
	cast(doc._Fld2727RRef as nvarchar(35)) Основание,
	case
		when doc._Fld3038 &gt; '30000101' then dateadd(yyyy,-2000,doc._Fld3038)
		else null
	end ПлановаяДатаДоставки,
cast(doc._Fld3070RRef as nvarchar(35)) СостояниеВМагазине,
	convert(datetime, convert(varchar, dateadd(yyyy,-2000,doc._Fld3037), 112)) ДатаПриемки,
	dateadd(yyyy,-2000,doc._Fld3037) ВремяПриемки,
	mag._Code as КодМагазина,
	tov._Code as КодТовара,
	ROUND(tab._Fld2733, 3) as Количество,
    ROUND(tab._Fld2780*tab._Fld2733, 2) as Сумма
FROM 
	[RSC].[dbo].[_Document2670] as doc (nolock)
inner join 
	[RSC].[dbo].[_Document2670_VT2728] as tab (nolock)
		on doc._IDRRef=tab._Document2670_IDRRef 
inner join 
	[RSC].[dbo].[_Reference114] as tov (nolock)
		on tab._Fld2730RRef=tov._IDRRef 
inner join 
	[RSC].[dbo].[_Reference129] as mag (nolock)
		on doc._Fld2725RRef=mag._IDRRef
where 
	dateadd(yyyy,-2000,doc.[_Date_Time]) between @dateN and @date
	and tab._Fld2733&lt;&gt;0 
	and doc._Fld3069 &lt;&gt; ' '
	and doc._Fld3037&gt;'30000101'</property>
